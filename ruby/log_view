#!/usr/bin/env ruby
require 'open3'
Open3.popen3("tail -f log/access.log") do |stdin, stdout, stderr, thread|
  while line = stdout.gets do
    t = line.split ' '
    src,status,method,dest,action,mime = [t[2],t[3],t[5],t[6],t[8],t[9]]
    puts "#{src} #{status} #{method} #{dest} #{action} #{mime}"
  end
end
